#include <cstdio>
using namespace std;
const double PI = 3.1415927;
 
int main(){
    int t = 1;
    double dia, rev, tim;
    while(scanf("%lf%lf%lf", &dia, &rev, &tim) && rev != 0){
        double dis, MPH;
        dis = (dia * rev * PI) / (5280 * 12);
        MPH = (dis * 3600 ) / tim;
        printf("Trip #%d: %.2lf %.2lf\n", t++, dis, MPH);
    }
    return 0;
}

