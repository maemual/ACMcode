#include <cstdio>
#include <string.h>
const int MAX_LEN = 100;

char a[MAX_LEN+5][MAX_LEN+5];

void reve(char * s)
{
	char p[MAX_LEN];
	int len = strlen(s);
	for (int i=0;i<len;i++)
	  p[i] = s[len-i-1];
	strcpy(s,p);
}
int main()
{
	int n;
	while (1)
	{
		scanf("%d",&n);
		if (n==0)
		  break;
		for (int i=0;i<n;i++)
		  scanf("%s",a[i]);
		for (int i=0;i<n;i++)
		  reve(a[i]);
		int minlen = MAX_LEN+5;
		for (int i=0;i<n;i++)
		{
			if (minlen > strlen(a[i]))
			  minlen = strlen(a[i]);
		}
		for (int i=0;i<
//		for (int i=0;i<n;i++)
//		  printf("%s\n",a[i]);
	}
	return 0;
}
