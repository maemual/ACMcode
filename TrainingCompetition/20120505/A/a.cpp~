#include <iostream>
#include <stdio.h>
#include <string>
#include <vector>
#include <algorithm>
using namespace std;
int length[2000];
vector<string> s;
void swap(string &p,string &q)  
{  
	string t=p; 
	p=q;  
	q=t;  
}
int partition(int ls,int lt)//快速排序算法中的一趟划分函数的实现  
{  
	int i,j;
	string temp;
	i=ls;j=lt;//对无序区b[s]到b[t]进行划分  
	temp=s[i];//基准记录  
	do  
	{	  
		while((s[j]<temp) && (i<j))  
			j--;//从右向左扫描，查找第一个关键字da于temp的记录  
		if(i<j)  {
			string tem = s[i];
			s[i] = s[j];
			s[j] = tem;
			
			int te = length[i];
			length[i] = length[j];
			length[j] = te;
			i++;
		}
		while((s[i]>=temp) && (i<j))  
			i++;//从左向右扫描，查找第一个关键字xiao于temp的记录  
		if (i<j)  {
			string tem = s[i];
			s[i] = s[j];
			s[j] = tem;
			
			int te = length[i];
			length[i] = length[j];
			length[j] = te;
			j--;
		}
		
	}while(i!=j);//i=j时 一次划分结束，基准记录到达最终位置  
	s[i]=temp;//最后将基准记录temp定位  
	return i;  
}  

void quick_sort(int hs,int ht)  
{  
	int i;  
	if (hs<ht)//只有一个或无记录时不须排序  
	{  
		i=partition(hs,ht);//对c[hs]到c[ht]进行一次划分  
		quick_sort(hs,i-1);//递归处理左区间  
		quick_sort(i+1,ht);//递归处理右区间  
	}  
}  


bool cmp(string s1,string s2)
{
	return s1 > s2;
}
int main()
{
	int m;
	while (cin >> m)
	{
		string stemp;
		s.clear();
		int max = -1;
		for (int i=0;i<2000;++i)
		  length[i] = 0;
		for (int i=0;i<m;++i)
		{
			cin >> stemp;
			s.push_back(stemp);
			length[i] = stemp.end() - stemp.begin();
			if (max < (stemp.end()-stemp.begin()))
			  max = (stemp.end()-stemp.begin());
		}
		for (int i = 0;i<s.size();++i)
		{
			while (s[i].size() < max)
			  s[i] += s[i][0];
			
			//cout <<length[i] << "  "<< s[i]<<endl;
		}	
		//sort(s.begin(),s.end(),cmp);
		quick_sort(0,m-1);
		//for (int i = 0;i<s.size();++i)
		//{
		//	cout <<length[i] << "  "<< s[i]<<endl;
		//}
		for (int i =0;i< s.size();++i)
		{
			for (int j=0;j<length[i];++j)
			{
				  printf("%c",s[i][j]);
			}
		}
		printf("\n");
	}
	return 0;
}
