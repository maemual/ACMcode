#include <cstdio>
#include <cstring>
const int MAX_LEN = 45;
int a[MAX_LEN];
int n;
long long sum;
void dfs(int k)
{
	if (k == n)
	{
		sum++;
		//for (int i=0;i<n;i++)
		//	printf("%3d",a[i]);
		//printf("\n");
		return;
	}
	for (int i=1;i<=3;i++)
	{
		if (k < 2)
		{
			a[k] = i;
		}else if ( i == 3 && a[k-1] == 2 && a[k-2] == 1)
		{
			continue;
		}else {
			a[k] = i;
		}
		dfs(k+1);
	}
}
int main()
{
	for (n=3;n<=40;n++)
	{
		memset(a,0,sizeof(a));
		sum = 0;
		dfs(0);
		printf("%lld\n",sum);
	}
	return 0;
}
