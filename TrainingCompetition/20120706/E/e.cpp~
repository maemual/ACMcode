#include <cstdio>
long long gcd(long long a,long long b)
{
	if (a%b==0) return b;
	return gcd(b,a%b);
}
long long lcm(long long a,long long b)
{
	return a/gcd(a,b)*b;
}

int main()
{
	int n;
	while (scanf("%d",&n) != EOF)
	{
		long long temp = 1;
		for (int i=1;i<=n;i++)
		{
			//int t= temp;
			//if (t > i)
			temp = lcm(temp,i);
			temp %= 99999989;
		}
		//int a,b;
		//scanf("%d%d",&a,&b);
		//printf("%d\n",gcd(a,b));
		printf("%lld\n",temp % 99999989);
	}
	return 0;
}
