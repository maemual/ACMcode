#include <iostream>
#include <stdio.h>
#include <string>
#include <sstream>
using namespace std;

int str2num(string s)
{
	int num;
	stringstream ss(s);
	ss >> num;
	return num;
}

int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		string s;
		cin >> s;
		string s2(s);
		bool conti = true;
		while (conti)
		{
			s = s2;
			s2 = "";
			int i = 0;
			while (i < s.size())
			{
				if (s[i]>='0' && s[i]<='9')
				{
					string sNum;
					int j=i;
					while (s[j]>='0' && s[j]<='9')
					{
						sNum += s2[j];
						++j;
					}
					int n = str2num(sNum);
	
					if (s[j] != '(')
					{
						string stemp;
						for (int k=0;k<n;++k)
						stemp += s[j];
						//s.insert(j,stemp,0,stemp.size());
						s2 += stemp;
					}else{
						int count1 = 1;
						int count2 = 0;
						int k = j;
						while (count1 != count2)
						{
							++k;
							if (s[k] == '(' )
							++count1;
							if (s[k] == ')')
							++count2;
						}
						string stemp(s,j+1,k-j-1);
						string st;
						for (int m=0;m<n;++m)
							st += stemp;
						//s.insert(k,st,0,st.size());
						s2 += st;
					}
				}
				++i;
			}
			
			conti = false;
			for (int i = 0;i<s2.size();++i)
			{
				if (s2[i] >'0' && s2[i] <= '9')
				{
					conti = true;
					break;
				}
			}
		}

		int i = 0;
		for (int i = 0;i<s2.size();++i)
		{
			if (s2[i]>='a' && s2[i]<='z')
			  printf("%c",s2[i]);
		}
		printf("\n");
	}
	return 0;
}
